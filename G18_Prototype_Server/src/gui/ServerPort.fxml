<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.TextArea?>

<Pane prefHeight="260.0" prefWidth="340.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.ServerPortFrameController"
      stylesheets="@ServerPort.css">   <children>
      <Label layoutX="30.0" layoutY="20.0" text="Port" />
      <TextField fx:id="portxt" layoutX="70.0" layoutY="16.0" prefWidth="120.0"/>

      <Button fx:id="btnDone" layoutX="210.0" layoutY="15.0" mnemonicParsing="false" onAction="#Done" text="Start" />
      <Button fx:id="btnExit" layoutX="270.0" layoutY="15.0" mnemonicParsing="false" onAction="#getExitBtn" text="Exit" />

      <TextArea fx:id="logArea" layoutX="20.0" layoutY="55.0"
                prefWidth="300.0" prefHeight="190.0"
                editable="false" wrapText="true"/>
   </children>
</Pane>
