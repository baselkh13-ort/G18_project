<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.MemberListController">

   <children>
      <TableView fx:id="tblMembers" layoutX="14.0" layoutY="14.0" prefHeight="330.0" prefWidth="772.0">
      	 <columns>
            <TableColumn fx:id="colMemberId" prefWidth="180.0" text="Member ID" />
            <TableColumn fx:id="colQrCode" prefWidth="160.0" text="QR Code" />
      		<TableColumn fx:id="colUserName" prefWidth="120.0" text="Username" />
      		<TableColumn fx:id="colFullName" prefWidth="150.0" text="Full Name" />
      		<TableColumn fx:id="colPhone" prefWidth="120.0" text="Phone" />
      		<TableColumn fx:id="colEmail" prefWidth="170.0" text="Email" />
   		 </columns>
	  </TableView>



      <Button layoutX="14.0" layoutY="355.0"
              onAction="#getBackBtn"
              prefHeight="30.0"
              prefWidth="120.0"
              text="Back" />
   </children>
</AnchorPane>
