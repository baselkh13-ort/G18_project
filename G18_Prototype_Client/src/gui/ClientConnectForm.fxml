<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="160.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ClientConnectFormController">
   <children>
      <Label layoutX="20.0" layoutY="20.0" text="Enter Server IP:" />
      <TextField fx:id="txtServerIP" layoutX="20.0" layoutY="45.0" prefWidth="210.0" text="Please enter an server IP address" />
      
      <Label fx:id="lblError" layoutX="20.0" layoutY="80.0" textFill="red" />
      
      <Button fx:id="btnConnect" layoutX="90.0" layoutY="110.0" mnemonicParsing="false" onAction="#getConnectBtn" text="Connect" />
   </children>
</AnchorPane>