<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.utils.OrderListController">
   <children>
      <Label fx:id="lblTitle" layoutX="22.0" layoutY="14.0" text="All Orders List" />
      
      <TableView fx:id="tblOrders" layoutX="22.0" layoutY="55.0" prefHeight="330.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="colOrderNum" prefWidth="70.0" text="Order number" />
          <TableColumn fx:id="colDate" prefWidth="100.0" text="Order date" />
          <TableColumn fx:id="colGuests" prefWidth="120.0" text="Number of guests" />
          <TableColumn fx:id="colConfirmationCode" prefWidth="100.0" text="Confirm Code" />
          <TableColumn fx:id="colSubscriber" prefWidth="90.0" text="Subscriber ID" />
          <TableColumn fx:id="colDateOfPlacingOrder" prefWidth="150.0" text="Date of placing order" />
        </columns>
      </TableView>
      
      <Button layoutX="22.0" layoutY="400.0" mnemonicParsing="false" onAction="#getBackBtn" text="Back to Menu" />
   </children>
</AnchorPane>